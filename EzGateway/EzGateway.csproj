<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Michal Diviš</Authors>
    <Company>Michal Diviš</Company>
    <Description>An abstraction layer for sending SMS (and other) messages. Including some implementations.</Description>
    <Copyright>Michal Diviš @ 2020</Copyright>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/michaldivis/EzGateway</RepositoryUrl>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Twilio" Version="5.45.1" />
  </ItemGroup>

</Project>
